{
  "name": "handlebarsHelper",
  "ns": "template",
  "description": "Handlebars Helper",
  "phrases": {
    "active": "Adding handlebar helper"
  },
  "ports": {
    "input": {
      "name": {
        "title": "Helper Name",
        "type": "string"
      },
      "fn": {
        "title": "Helper Function",
        "type": "function",
        "description": "The Helper function"
      }
    },
    "output": {
      "handlebars": {
        "title": "Handlebars",
        "type": "function"
      }
    }
  },
  "dependencies": {
    "npm": {
      "handlebars": "latest"
    }
  }
}